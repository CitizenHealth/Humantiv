{
  "name": "Humantiv",
  "version": "1.23.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/CitizenHealth/Humantiv.git"
  },
  "keywords": [],
  "author": "Nabyl Bennouri <nabyl@citizenhealth.io> (https://citizenhealth.io)",
  "bugs": {
    "url": "https://citizenhealth.atlassian.net/projects/HSA/issues"
  },
  "homepage": "https://citizenhealth.io",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "./scripts/clean_build.sh",
    "postinstall": "cd node_modules/react-native; exec ./scripts/ios-install-third-party.sh; cd third-party/glog-0.3.4; exec ./configure; cd ../../../..",
    "test": "npx jest --verbose",
    "test:watch": "jest --watch --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e:ios": "detox test -c ios.sim.debug",
    "test:e2e:ios:build": "detox build",
    "test:e2e:android": "detox test -c android.emu.debug",
    "test:update": "npm test -- -u",
    "storybook": "start-storybook -p 7007",
    "device": "react-native run-ios --device",
    "android:log": "react-native log-android",
    "ios:release": "react-native run-ios --configuration Release",
    "ios:log": "react-native log-ios",
    "ios:sim": "./scripts/start_ios_simulator.sh",
    "ios:device": "react-native run-ios --device",
    "android": "react-native run-android",
    "android:sim": "./scripts/start_android_simulator.sh",
    "metro": "npm start -- --reset-cache"
  },
