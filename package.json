  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-ondevice-knobs": "^5.3.17",
    "@storybook/addon-ondevice-notes": "^5.3.17",
    "@storybook/addon-options": "5.3.17",
    "@storybook/addon-storyshots": "5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/channel-postmessage": "5.3.17",
    "@storybook/channel-websocket": "5.3.17",
    "@storybook/channels": "5.3.17",
    "@storybook/client-logger": "5.3.17",
    "@storybook/components": "5.3.17",
    "@storybook/core": "5.3.17",
    "@storybook/node-logger": "5.3.17",
    "@storybook/react-native": "^5.3.17",
    "@storybook/react-native-server": "^5.3.17",
    "@storybook/ui": "^5.3.17",
    "@types/convert-units": "^2.3.0",
    "@types/lodash": "^4.14.149",
    "@types/react-native-vector-icons": "^6.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.2.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-new-target": "^7.0.0-alpha.14",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "7.0.0-beta.3",
    "detox": "^16.0.1",
    "jest": "^25.2.3",
    "metro-react-native-babel-preset": "^0.59.0",
    "prop-types": "^15.7.1",
    "react-dom": "^16.8.6",
    "react-native-bundle-visualizer": "^2.1.1",
    "react-test-renderer": "^16.8.6",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "redux-devtools": "^3.5.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|mobx)",
      "!node_modules/react-runtime"
    ],
    "setupFiles": [
      "./__mocks__/react-native-google-signin.js",
      "./__mocks__/react-native-firebase.js"
    ],
    "automock": false
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Humantiv.app",
        "build": "xcodebuild -project ios/Humantiv.xcodeproj -scheme Humantiv -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_24"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_5X_API_26"
      }
    },
    "resolutions": {
      "@react-native-community/cli": "^2.8.3"
    },
    "test-runner": "jest",
    "specs": "",
    "runner-config": "e2e/config.json"
  }
}
